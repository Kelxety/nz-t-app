{
  "$schema": "http://json-schema.org/schema",
  "$id": "BmSchematic",
  "title": "My Component Schema",
  "type": "object",
  "description": "Creates a new service.",
  "properties": {
    "name": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 0
